{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "babel-plugin-transform-object-assign",
    "dynamic-import-node",
    "transform-class-properties",
    "lodash"
  ],
  "env": {
    "local-webpack": {
      "presets": [
        "react",
        [
          "es2015",
          {
            "modules": false
          }
        ],
        "stage-0"
      ],
      "plugins": [
        "transform-object-rest-spread",
        "babel-plugin-transform-object-assign",
        "transform-class-properties",
        "syntax-dynamic-import",
        "react-hot-loader/babel",
        "lodash"
      ]
    },
    "webpack": {
      "presets": [
        "react",
        [
          "es2015",
          {
            "modules": false
          }
        ],
        "stage-0"
      ],
      "plugins": [
        "transform-object-rest-spread",
        "babel-plugin-transform-object-assign",
        "transform-class-properties",
        "syntax-dynamic-import",
        "lodash"
      ]
    }
  }
}